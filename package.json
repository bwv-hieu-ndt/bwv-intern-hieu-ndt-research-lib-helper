{
  "name": "hieundt-nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/main.ts",
    "build": "tsup",
    "start": "node ./dist/main.js",
    "migration:create": "npx sequelize-cli model:generate",
    "migrate": "env-cmd npx sequelize-cli db:migrate",
    "migrate:rollback": "env-cmd npx sequelize-cli db:migrate:undo"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "dotenv": "^16.4.6",
    "express": "^4.21.1",
    "mysql2": "^3.11.5",
    "sequelize": "^6.37.5"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
